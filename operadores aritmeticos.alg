algoritmo "operadores aritmeticos"
// Função :
// Autor :
// Data : 18/11/2020
// Seção de Declarações 
var
   resultado, valor1, valor2, valor3: real

inicio
// Seção de Comandos
   valor1 := 10
   valor2 := 10
   valor3 := 10
   
   resultado := ((valor1 + valor2 + valor2) / valor3) % 2
   escreval ("Resultado: ", resultado)

fimalgoritmo
